﻿local sudo = {
'شما سودو هستید',
'سودویی عزیزم',
'سودویی دیگه 😐',
'اقا سودویی😐',
'بخدا سودویی😐',

},
'local' member = {
'شما ممبر هستید',
'داداچ گفتم ممبری😐',
'ممبری دیگه /:',
 'صد دف گفتم ممبری;😐',
'بسه گفتم ممبری بگو چشم😒\nکانال رسمی بیوند تیم \n@BeyondTeam'

  },
'local' function run(msg, matches)
if is_sudo(msg) then
return sudo[math.random(#sudo)]
else
return member[math.random(#member)]
end
end
return {
  patterns = {
    "^من کیم",
  },
  run = run
}
